{
  "compilerOptions": {
    "target": "es5",
    "lib": [
      "es5",
      "es2015.iterable",
      "es2015.promise",
      "es2015.collection",
      "es2015.symbol.wellknown",
      "esnext.asynciterable"
    ],
    "downlevelIteration": true,
    "importHelpers": true,
    "module": "commonjs",
    "strict": true,
    "declaration": true,
    "sourceMap": true,
    "experimentalDecorators": true,
    "emitDecoratorMetadata": true,
    "rootDir": "./src",
    "outDir": "./build"
  }
}
